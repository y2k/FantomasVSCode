{
  "name": "fantomasvscode",
  "displayName": "Fantomas",
  "description": "FSharp source code formatter",
  "version": "0.0.1",
  "publisher": "zelay",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.fntms.format"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.fntms.format",
        "title": "Fantomas: Format F#"
      }
    ]
  },
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf bin && rm -rf node_modules && rm -rf obj && rm -rf out && rm -rf packages"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "fable-import-vscode": "0.1.0-narumi-2",
    "fable-loader": "^1.0.0",
    "fable-powerpack": "^1.0.0-narumi-5",
    "vscode": "^1.1.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "tmp": "0.0.31"
  }
}